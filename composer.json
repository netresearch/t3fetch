{
    "name": "visuellverstehen/t3fetch",
    "type": "typo3-cms-extension",
    "description": "Fetches a website (including all subpages), so the TYPO3 cache gets filled.",
    "license": "MIT",
    "authors": [
        {
            "name": "visuellverstehen",
            "email": "kontakt@visuellverstehen.de",
            "homepage": "https://www.visuellverstehen.de"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.typo3.org/"
        }
    ],
    "require": {
        "typo3/cms-core": ">=7.6 <=9.5",
        "php": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "VV\\T3fetch\\": "Classes/"
        }
    },
    "scripts": {
        "extension-release": [
            "rm .gitignore",
            "rm .travis.yml",
            "rm -rf .git",
            "rm -rf typo3",
            "rm -rf vendor"
        ]
    }
}
